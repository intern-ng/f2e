doctype html
include utility
html
  head
    block head
      block meta
        meta(charset="utf-8")
        +viewport
        +ua-compatible
      block favicon
      block style
        +stylesheet('//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css')
  body
    block content
    block script
      +javascript('//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
      +javascript('//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js')
